        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.mapper.AdminMapper">

    <resultMap id="BaseResultMap" type="com.test.bean.po.Admin">
            <id property="adminId" column="admin_id" jdbcType="INTEGER"/>
